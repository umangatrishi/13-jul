#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        int number;
        long long factorial = 1;
        cin >> number;
        for (int i = 1; i <= number; ++i) {
            factorial *= i;
        }
        cout<< factorial << endl;
    }
    return 0;
}
